  "testingProtocol": {
    "duration": "2-4 weeks",
    "methodology": "Observational study with feedback surveys",
    "dataCollection": [
      "Initial questionnaire (experience, goals)",
      "Weekly feedback surveys",
      "Suggestion acceptance/rejection rates",
      "Injury or adverse event tracking",
      "Final satisfaction survey"
    ],
    "metrics": [
      {
        "metric": "Suggestion Acceptance Rate",
        "target": ">70%",
        "calculation": "Accepted suggestions / Total suggestions"
      },
      {
        "metric": "User Satisfaction",
        "target": ">4.0/5.0",
        "calculation": "Average rating from post-test survey"
      },
      {
        "metric": "Perceived Accuracy",
        "target": ">80% agree",
        "calculation": "Users who rate suggestions as accurate/appropriate"
      },
      {
        "metric": "Time Saved",
        "target": ">50% report time savings",
        "calculation": "Users reporting faster workout logging"
      },
      {
        "metric": "Injury/Adverse Events",
        "target": "0",
        "calculation": "Count of reported issues"
      }
    ]
  },
  "feedbackQuestions": [
    {
      "question": "How accurate were the weight suggestions?",
      "type": "scale",
      "scale": "1-5 (Very Inaccurate to Very Accurate)"
    },
    {
      "question": "Did the suggestions help you progress safely?",
      "type": "scale",
      "scale": "1-5 (Strongly Disagree to Strongly Agree)"
    },
    {
      "question": "How often did you accept the suggestions?",
      "type": "multiple-choice",
      "options": ["Always", "Usually", "Sometimes", "Rarely", "Never"]
    },
    {
      "question": "Did the feature save you time?",
      "type": "boolean",
      "options": ["Yes", "No"]
    },
    {
      "question": "What improvements would you suggest?",
      "type": "open-ended"
    },
    {
      "question": "Would you recommend this feature to others?",
      "type": "scale",
      "scale": "1-5 (Definitely Not to Definitely Yes)"
    }
  ]
}
